<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- Restore groups to avoid FK violation during upgrade -->
    <!-- We can mark them as nodrop/noupdate later or remove references manually -->

    <record id="group_real_estate_user" model="res.groups">
        <field name="name">Real Estate User (Deprecated)</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
    
    <record id="group_real_estate_manager" model="res.groups">
        <field name="name">Real Estate Manager (Deprecated)</field>
        <field name="implied_ids" eval="[(4, ref('group_real_estate_user'))]"/>
    </record>

</odoo>
